{
    "name": "Stream Viewer",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Streams from multiple games in one place.",
    "browser_action": {
        "default_icon": "img/icon/twitch-icon.png",
        "default_popup": "index.html",
        "default_title": "StreamViewer"
    },
    "permissions": [
        "*://*/*",
        "tabs",
        "storage"
    ]
}
